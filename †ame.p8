--colide
function dist_to_col(x,y)
local result = {}
local c = {} -- colide position

c.x= 8*flr((hero.x+7)/8)+(x*8)
c.y= 8*flr((hero.y+7)/8)+(y*8)

if is_b(c.x,c.y) then
 result.x= abs(c.x-hero.x)-7
 result.y= abs(c.y-hero.y)-7
elseif is_b(c.x,c.y+8) then
 result.x= abs(c.x-hero.x)-7
 result.y= abs(c.y-hero.y+8)-7
elseif is_b(c.x+8,c.y) then
 result.x= abs(c.x-hero.x+8)-7
 result.y= abs(c.y-hero.y)-7
elseif is_b(c.x+8,c.y+8) then
 result.x= abs(c.x-hero.x+8)-7
 result.y= abs(c.y-hero.y+8)-7
 end
 
 return result
end

function is_b(x,y) -- is block

map_n = mget(x/8,y/8)
return fget(map_n,0)
end
