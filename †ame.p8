--init
function _init()
hero = {}
col = {} -- colide position
hero.hp = 3
hero.x = 4*8
hero.y = 24
hero.x_dir = 1 --1 >= dir >= -1
hero.y_dir = 0 --1 >= dir >= -1
hero.flip = false

hero.sx = 1 -- speed
hero.sy = 1 -- speed
hero.on_flore = false

i = 0
max_frame = 3
speed = 5

h_g = 0 -- hero gravity get() 
end


-->8
--update
function _update()
key_control()
camera(hero.x-64,hero.y-64)
end


-->8
--draw
function _draw()
cls()
map()
print(frame,hero.x-10,hero.y-10)
print(hero.sy,hero.x-14,hero.y-6)

_draw_hero()
draw_hero_hp()
end

function _draw_hero()
i += 1/speed
frame = 0

if hero.sy==0 then
frame = flr(i) % max_frame
end

sspr (frame*16,0,
16,16,
hero.x,hero.y-((2-frame)*2)+1,16,16,
hero.flip,false)
if i >= max_frame then
 i = 0
 end
end
-->8
--movement
function key_control()

if(hero.sx == 0) then
hero.x_dir = -1*hero.x_dir
hero.flip = not hero.flip
--check_flip()
end
hero.sx = 1


if(hero.sy == 0) then
 h_g = 1
end

if(btnp(⬆️)
 and hero.sy == 0 
 and hero.on_flore) then
 h_g = -4
end
 
h_g += 0.2
hero.y_dir = sgn(h_g)
hero.sy = abs(h_g)
 
hero_move()
end

function hero_move()
xd =flr((hero.x_dir+1)/2)
yd =flr((hero.y_dir+1)/2)

col = dist_to_col(hero.x_dir,0)
if (col.x  ~= nil and 
col.x-(8*xd) <= hero.sx) then
 hero.sx = 0
 end
 
col = dist_to_col(0,hero.y_dir)
if (col.y ~= nil and 
    col.y-(8*yd) <= hero.sy) then
 check_flore()
 hero.sy = 0
 end
 
hero.x += hero.x_dir*hero.sx
hero.y += hero.y_dir*hero.sy
end

--only hero
function check_flore()
 hero.on_flore = false
 if hero.y_dir == 1 then
  hero.on_flore = true
  end
end

