--movement
function key_control()

--save zone
col = dist_to_col(0,0,0)
if col ~= nil then
update_point(hero)
end

--thorns
col = dist_to_col(0,0,2)
if ((col  ~= nil and
col.x <= hero.sx) or
(col  ~= nil and
col.y <= hero.sy))
and not save_form
 then
 hero.hp-=1
 save_form = true
end
update_save_form()

jump_s_b = false
--springboards
col = dist_to_col(0,0,3)
if (col  ~= nil and
col.y <= hero.sy) or 
(col  ~= nil and
col.x <= hero.sx) 
 then
 hero.sy = 0 - (jump_power*1.55)
 jump_s_b = true 
end
update_tr(hero,jump_s_b)
hero_move()
end


function hero_move()


colx = dist_to_col(sgn(hero.sx),0,1)
coly = dist_to_col(0,sgn(hero.sy),1)
hero.sy += gravity

if coly ~= nil then

 if coly.y <= hero.sy then
 hero.sy = -2*sgn(hero.sy)
else
  hero.sy = coly.y-9 <= 0 and 0 
                     or hero.sy
  end
end



             
xd = abs(flr((hero.sx-1)/2)) //kostil


if colx ~= nil and
colx.x+(4*xd) <= hero.sx
 then
if (hero.sy == 0  and
 coly.y-9 >= 0)
  then
  hero.sx *= -1
  else
  xpow = 0
  end
double_jump() 
end

xpow = (colx == nil or 
 hero.on_flore) and 1 
or xpow
       

coly = dist_to_col(0,-1,1)
if(btnp(⬆️)
 and hero.on_flore)
 and coly == nil then
 sfx(55)
 hero.sy -= jump_power
end

hero.x += hero.sx*xpow
hero.y += hero.sy
--
coly = dist_to_col(0,1,1)
hero.on_flore = hero.sy == 0 and
 coly ~= nil and coly.y-9 >= 0
--
normalize_hero_pos()
end

function normalize_hero_pos()
if hero.sy == 0  then
 hero.y = flr(hero.y/8)*8
end
end

function double_jump()

coly = dist_to_col(0,-1,1)
if btnp(⬆️)  and hero.sy > 0 and
coly == nil
then
hero.sy -= jump_power*1.25
hero.sx *= -1
xpow = 1
end
end
