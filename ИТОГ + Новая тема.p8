function find_torch_arr()

 index = 1
 result = {}
 
 for x = 0,127 do
  for y = 0,63 do
   
   if is_b(x*8,y*8,5) then
    result[index*3-2] = x
    result[index*3-1] = y
    result[index*3] = flr(rnd(3))
    index += 1
   end
    
  end
 end
 
 return result
 
end







function get_sumb_index(
           str,start_sub,sumb)
  result = -1
  for i=start_sub, #str do
   local s = sub(str,i,i)
   if s == sumb then
      result = i 
      break
   end
  end
  return result
end

function torch_draw(torch_arr)
 spd = 10
 for i = 1, #torch_arr, 3 do
  mset(torch_arr[i],
       torch_arr[i+1],
       69+flr(my_time/spd
        +torch_arr[i+2])%3)
       
 end
end


function find_point_zone()
local result = {}
m = 0
for i = 0, 127,1 do

for j = 0, 63,1 do

if  fget(mget(i,j),0) then
local examp = {}
examp.x = i
examp.y = j
examp.is_active = false
result[m] = examp
m += 1
end

end

end

return result
end

function off_points()

for i = 0, #point_zone, 1 do
point_zone[i].is_active = false
end

end


