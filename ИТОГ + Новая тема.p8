--init
function _init()
hero = {}
movement = {}
col = {} -- colide position
max_colide_count = 2
hero.hp = 3
hero.x = 1*8
hero.y = 61*8
movement.x = hero.x
movement.y = hero.y
hero.sx = 1 -- speed
hero.sy = 1 -- speed
jump_power = 4
hero.on_flore = false
gravity = 0.3
time_damage = 1
second_save = 3
save_ef_speed = 21
save_form = false
i = 0
max_frame = 3
speed = 5
last = 0
music(0)
b = 0


torch_arr = find_torch_arr()
point_zone = find_point_zone() 
my_time=0
mm_pos = 0
mm_status = 0
mm_max_button = 2
tr_str = ""
cur_point = 0
xpow = 1
end

