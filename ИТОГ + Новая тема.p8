--collide
function collide(dir_x,dir_y,n)
local result = {}
c = {} -- colide position
c.x= 8*flr((hero.x+7)/8)+(dir_x*8)
c.y= 8*flr((hero.y+7)/8)+(dir_y*8)

for i=0,1,1 do
 for j=0,1,1 do
  if is_b(c.x+(8*i),c.y+(8*j),n)
  then
  //result.x= abs(c.x-hero.x+(8*i))-7-(8*xd)
  //result.y= abs(c.y-hero.y+(8*j))-7

  result.x= c.x+(8*i)
  result.y= c.y+(8*j)
  return result
   end
  end
 end
 
 return result
end

function dist_to_col(dir_x,dir_y,n)
local result = {}
local p = {}
p.x = 0
p.y = 0
p = collide(dir_x,dir_y,n)
xd =flr((dir_x+1)/2)

if p.x ~= nil and p.y ~= nil then
result.x= abs(p.x-hero.x)-7-(8*xd)
result.y= abs(p.y-hero.y)-7
else
result = nil
end

return result
end

function is_b(x,y,n) -- is block
map_n = mget(x/8,y/8)
return fget(map_n,n)
end
