function find_torch_arr()

 index = 1
 result = {}
 
 for x = 0,127 do
  for y = 0,63 do
   
   if is_b(x*8,y*8,5) then
    result[index*3-2] = x
    result[index*3-1] = y
    result[index*3] = flr(rnd(3))
    index += 1
   end
    
  end
 end
 
 return result
 
end







function get_sumb_index(
           str,start_sub,sumb)
  result = -1
  for i=start_sub, #str do
   local s = sub(str,i,i)
   if s == sumb then
      result = i 
      break
   end
  end
  return result
end





function torch_draw(torch_arr)
 spd = 10
 for i = 1, #torch_arr, 3 do
  mset(torch_arr[i],
       torch_arr[i+1],
       69+flr(my_time/spd
        +torch_arr[i+2])%3)
       
 end
end



--for tramplines
function add_tr_anim(x,y)

  tr_str = tr_str .. tostr(x) .. 
        "," .. tostr(y) ..   
        "," .. tostr(my_time) ..
        " "
end


function check_tr_anim()
  
  temp = ""
  
  
  while tr_str != "" do
  index = get_sumb_index(tr_str,
                          0, " ")
  sub_str = sub(tr_str,0,index)
  
  sh_be_del = update_tr(sub_str)
  
  tr_str = sub(tr_str,index+1)
   
  if sh_be_del  then 
    
    temp = temp .. sub_str
    
  end                     
    
  end
  
  tr_str = temp
  
end





function update_tr(sub_str)
  
  result = true
  
  fst_index = get_sumb_index(
      sub_str, 0, ',')
      
  sec_index = get_sumb_index(
      sub_str, fst_index+1, ',')
  
   x = sub(sub_str,
                0,fst_index-1)
   y = sub(sub_str,
                fst_index+1,sec_index-1)
   str_time = sub(sub_str,
                       sec_index+1)
   
   diff = my_time - str_time
   
   if diff > 20 then
     result = false
   end
   
   if mget(x,y) == 113 or mget(x,y) == 112 
   then
    if (diff < 3 or diff > 17)
    then
   
    mset (x,y,112)
    mset (x,y-1,68)
   
   else 
   
    mset (x,y, 113)
    mset (x,y-1,114) 
    
    end 
   end
   
   return result                  
end
